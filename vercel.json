{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src":"public/**",
      "use":"@vercel/static"
    }
  ],
  "routes": [
    
    {
      "src":"/transactions/",
      "dest":"app.js"
    },
    {
      "src": "/users/",
      "dest": "app.js"
    },
    {
      "src":"/",
      "dest":"public/index.html"
    },
    {
      "src":"/login",
      "dest":"public/index.html"
    },
    {
      "src":"/register",
      "dest":"public/index.html"
    }
    
  ]
 
  
}